Int [3] const idx = [0, 1, 2];

Int [3] const a = [1, 2, 3];

Int [3] b;

Int i = 0;
for (Int e in a) {
    b[i] = e * 2;
    i = i + 1;
};

(Int, Int) [3] c;

# Int i in idx can be replaced by Int i in [0 to 3], but by, to and ... are not implemented yet.
for (Int e in a, Int f in b, Int i in idx)
    c[i] = (e, f);

Int [3] d;

for ((Int e, Int f) in c, Int i in idx)
    d[i] = e + f;

return d[2];
